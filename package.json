{
  "name": "vue-3-ssr-fetchdata",
  "version": "0.1.0",
  "description": "A Vue 3 POC with SSR and CSR data fetching via a composable",
  "main": "server.js",
  "keywords": [
    "vue",
    "ssr",
    "csr",
    "vue-router",
    "pinia"
  ],
  "dependencies": {
    "express": "^4.19.2",
    "pinia": "^2.2.1",
    "vue": "^3.4.37",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@stylistic/eslint-plugin": "^2.6.2",
    "@vitejs/plugin-vue": "^5.1.2",
    "eslint": "^9.9.0",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "vite": "^5.4.0"
  },
  "lint-staged": {
    "**/*.{js,vue}": [
      "eslint --fix"
    ]
  },
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepare": "husky install",
    "start": "node server.js"
  },
  "type": "module"
}
